
{
 	"steps": [
 		["services.preprocesing", "RegExpRemover", "url_hastags_remover"],
 		["services.preprocesing", "TweeterTokenizerSvc", "tweeter_tokenizer"],
 		["services.preprocesing", "StopWordsRemoverSvc", "remove_stopwords"]
 	],
 	"memory": false,
 	"url_hastags_remover_conf": {
 		"kwargs": {
 		    "wrapper_persist_removed_urls": true,
		    "wrapper_remove_urls": true,
		    "wrapper_remove_htags": true
 		}
 	},
 	"remove_stopwords_conf": {
 		"args": ["english"],
 		"kwargs": {}
 	},
 	"tweeter_tokenizer_conf": {
 		"args": [],
 		"kwargs": {
 			"preserve_case": true,
 			"strip_handles": true
 		}
 	}

}
