
{
    "steps": [
	["services.preprocesing", "UrlRegExpRemoverSvc",      "remove_urls"],
	["services.preprocesing", "HashtagRegExpRemoverSvc",  "remove_hashtags"],
	["services.preprocesing", "PunktuationExpRemoverSvc", "remove_punktuations"],
	["services.preprocesing", "TweeterTokenizerSvc",      "tokenize"],
	["services.preprocesing", "StopWordsRemoverSvc",      "remove_stopwords"],
	["services.preprocesing", "EmojiReplacerSvc",         "replace_emojis"]
    ],
    "memory": false,
    "remove_urls_conf": {
 	"kwargs": {
 	    "wrapper_persist_removed_urls": true
 	}
    },
    "remove_hashtags_conf": {
 	"kwargs": {
	}
    },
    "remove_stopwords_conf": {
	"kwargs": {
	    "wrapper_language": "english"}
    },
    "tokenize_conf": {
 	"kwargs": {
 	    "preserve_case": true,
 	    "strip_handles": true
 	}
    }
    
}
