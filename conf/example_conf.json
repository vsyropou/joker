
{
 	"steps": [
 		["services.preprocesing", "UrlRemover", "url_remover"],
 		["services.preprocesing", "TweeterTokenizerSvc", "tweeter_tokenizer"],
 		["services.preprocesing", "StopWordsRemoverSvc", "remove_stopwords"]
 	],
 	"memory": false,
 	"url_remover_conf": {
 		"kwargs": {
 			"wrapper_persist_removed_urls": true
 		}
 	},
 	"remove_stopwords_conf": {
 		"args": ["english"],
 		"kwargs": {}
 	},
 	"tweeter_tokenizer_conf": {
 		"args": [],
 		"kwargs": {
 			"preserve_case": true,
 			"strip_handles": false
 		}
 	}

}
